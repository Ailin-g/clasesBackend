{{#if hayProductos}}

<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Nombre</th>
      <th scope="col">Precio</th>
      <th scope="col">Thumbnail</th>
    </tr>
  </thead>
  <tbody>
      {{#each listaProductos}}
        <tr>
        <th scope="row">{{this.id}}</th>
        <td>{{this.nombre}}</td>
        <td>{{this.precio}}</td>
        <td>{{this.thumbnail}}</td>
        </tr>
      {{/each}}
    </tbody>
</table>

{{/if}}